#!/usr/bin/php
<?php
define('ENV', 'WORKER_' . getmypid());

$configFile = require 'cli.php';

try {
    \Maestroprog\Saw\Saw::instance()
        ->init($configFile)
        ->instanceWorker()
        ->start();
} catch (Exception $e) {
    echo "\e[1;31m{$e->getMessage()}\033[0m" . PHP_EOL;
    exit(127);
}
